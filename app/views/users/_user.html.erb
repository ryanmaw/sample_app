<li>
	<%= link_to user.name, user %>
	<% if current_user.admin?  && !current_user?(user)%>
	<%# if the current user is an admin and we it isn't there name in the list (no delete link for self) %>
	| <%= link_to "delete", user, method: :delete, data: { confirm: "Are you sure you want to delete this user?\nUser Name: #{user.name}" } %>
	<% end %>
</li>
